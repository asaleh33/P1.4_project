    "def distmatrix(N, dist, x_train):\n",
    "    D = np.zeros((N,N))\n",
    "    for i, j in itertools.product(xrange(N), xrange(i,N)):\n",
    "        D[i][j] = dist(x_train[i], x_train[j])\n",
    "        D[j][i] = D[i][j]\n",
    "    return D        "
   ]
